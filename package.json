{
  "name": "passport-easydb",
  "version": "0.1.4",
  "description": "easydb authentication strategy for Passport.",
  "keywords": [
    "passport",
    "easydb",
    "auth",
    "authn",
    "authentication",
    "identity"
  ],
  "author": "Stefan Peters",
  "repository": {
    "type": "git",
    "url": "http://github.com/gbv/passport-easydb.git"
  },
  "bugs": {
    "url": "http://github.com/gbv/passport-easydb/issues"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "type": "module",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint",
    "fix": "eslint --fix",
    "lint-staged": "lint-staged",
    "release": "npm test && git pull && npm version $SEMVER && git push --follow-tags",
    "release:patch": "SEMVER=patch npm run release",
    "release:minor": "SEMVER=minor npm run release",
    "release:major": "SEMVER=major npm run release"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "devDependencies": {
    "eslint": "~9.11",
    "eslint-config-gbv": "~2.1",
    "lint-staged": "^15.2.10",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "passport-strategy": "^1.0.0"
  }
}
